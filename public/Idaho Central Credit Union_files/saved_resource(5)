(function() { const accId = "885055130827083336"; const webId = "3032110629069658509"; const depId = "1910685051822057888"; const winId = "7717989440186623200"; var bccbId = "orbital_chat"; if (typeof (window._bcvma) === "undefined") { window._bcvma = window._bcvma || []; _bcvma.push(["setAccountID", accId]); _bcvma.push(["setParameter", "WebsiteID", webId]);_bcvma.push(["addText", {type: "chat", department: depId, window: winId, available: "Live Chat", unavailable: "Unavailable", id: bccbId}]); var bcLoad = function(){ if(window.bcLoaded) return; window.bcLoaded = true; var vms = document.createElement("script"); vms.type = "text/javascript"; vms.async = true; vms.src = ("https:"==document.location.protocol?"https://":"http://") + "vmss.boldchat.com/aid/" + accId + "/bc.vms4/vms.js"; var s = document.getElementsByTagName("script")[0]; s.parentNode.insertBefore(vms, s); }; if(window.pageViewer && pageViewer.load) pageViewer.load(); else if(document.readyState=="complete") bcLoad(); else if(window.addEventListener) window.addEventListener("load", bcLoad, false); else window.attachEvent("onload", bcLoad); }})();